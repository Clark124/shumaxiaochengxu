/* miniprogram/pages/userCenter/userCenter.wxss */
.userinfo{
  text-align: center;
}
.userinfo-block-avatar{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  margin: 0 auto;
  overflow: hidden;

}
.userinfo-avatar{
  width: 200rpx;
  height: 200rpx;
  margin: 20rpx;
  border-radius: 50%;
  background-size: cover;
  background-color: white;
}
.userinfo-avatar[size] {
  width: 200rpx;
}
.username{
  text-align: center;
  margin-top: 20rpx;
}
.nav-my-publish{
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx;
  border-bottom: 1rpx solid #eee;
}
