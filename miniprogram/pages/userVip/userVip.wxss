/* miniprogram/pages/userVip/userVip.wxss */
page{
  background: #f5f5f5;
  padding-bottom: 30rpx;
}
.info-wrapper{
  background: #fff;
  height: 320rpx;
  width: 100%;
  padding: 30rpx 25rpx;
  box-sizing: border-box;
  position: relative;
  
}
.info-bg{
  position: absolute;
  width: 700rpx;
  height: 260rpx;
  left: 25rpx;
  top: 30rpx;

}
.userinfo{
  display: flex;
  align-items: center;
  position: absolute;
  left: 70rpx;
  top: 90rpx;
  z-index: 1;
}
.avatar{
  width: 140rpx;
  height: 140rpx;
  border-radius: 70rpx;
  margin-right: 20rpx;
}
.nikcname{
  display: flex;
  align-items: center;
}
.name{
  font-size: 32rpx;
  color: #fff;
  margin-right: 24rpx;
}
.status{
  font-size: 24rpx;
  color: #EDBA56;
  margin-right: 14rpx;
}
.vip-icon{
  width: 35rpx;
  height: 23rpx;
}
.vip-date{
  font-size: 24rpx;
  color: #fff;
  margin-top: 20rpx;
}
.table{
  width: 700rpx;
  margin-left: 25rpx;
}